<h1>Listing stores</h1>

<table>
  <tr>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>State</th>
    <th>Zip</th>
    <th>City</th>
    <th>Address</th>
    <th>Advertiser</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stores.each do |store| %>
  <tr>
    <td><%= store.latitude %></td>
    <td><%= store.longitude %></td>
    <td><%= store.state %></td>
    <td><%= store.zip %></td>
    <td><%= store.city %></td>
    <td><%= store.address %></td>
    <td><%= store.advertiser_id %></td>
    <td><%= link_to 'Show', store %></td>
    <td><%= link_to 'Edit', edit_store_path(store) %></td>
    <td><%= link_to 'Destroy', store, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Store', new_store_path %>
